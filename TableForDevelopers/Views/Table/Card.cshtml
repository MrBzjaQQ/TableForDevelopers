@model TableForDevelopers.Models.CardModel
         <div class="modal-content">
             <div class="modal-header">
                  <h3 class="modal-title">Карточка №@Model.CardID</h3>
                 <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                     <span aria-hidden="true">&times</span>
                 </button>
                
             </div>
             <div class="modal-body">
                 <form>
                     Здесь должны быть подробные параметры и описание карточки
                 </form>
             </div>
             <div class="modal-footer">
                 <button id="CancelLoginButton" class="btn btn-secondary" value="Cancel" data-dismiss="modal">Отмена</button>
                 <button id="AcceptLoginButton" type="submit" class="btn btn-primary">Войти</button>
             </div>
         </div>
